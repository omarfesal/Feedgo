<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>vimeoApi</servlet-name>
        <servlet-class>apis.vimeoApi</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>github_api</servlet-name>
        <servlet-class>apis.github_api</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>slideshare_api</servlet-name>
        <servlet-class>apis.slideshare_api</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>registration</servlet-name>
        <servlet-class>DatabaseConnecton.registration</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>DatabaseConnecton.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>DatabaseConnecton.logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>profile</servlet-name>
        <servlet-class>control.profile.profile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>setting</servlet-name>
        <servlet-class>DatabaseConnecton.setting</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>slideSharePost</servlet-name>
        <servlet-class>control.slideSharePost</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ajax</servlet-name>
        <servlet-class>DatabaseConnecton.Setting_ajax</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VimeoPost</servlet-name>
        <servlet-class>control.VimeoPost</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>githubPost</servlet-name>
        <servlet-class>control.githubPost</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Rate_ajax</servlet-name>
        <servlet-class>control.Rate_ajax</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>vimeoApi</servlet-name>
        <url-pattern>/vimeoApi</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>github_api</servlet-name>
        <url-pattern>/github_api</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>slideshare_api</servlet-name>
        <url-pattern>/slideshare_api</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>registration</servlet-name>
        <url-pattern>/registration</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>profile</servlet-name>
        <url-pattern>/profile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>setting</servlet-name>
        <url-pattern>/setting</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>slideSharePost</servlet-name>
        <url-pattern>/slidePost</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ajax</servlet-name>
        <url-pattern>/ajax</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VimeoPost</servlet-name>
        <url-pattern>/VimeoPost</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>githubPost</servlet-name>
        <url-pattern>/githubPost</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Rate_ajax</servlet-name>
        <url-pattern>/Rate_ajax</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
